{
    "manifest_version": 3,
    "description": "Simple Korean-English dictionary",
    "name": "Naver Dictionary",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon128.png"
    },
    "host_permissions": [
        "https://*.naver.com/"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}